    game.StarterGui:SetCore("SendNotification", {
        Title = "âš¡ Aimbot Ù…ÙØ¹Ù„",
        Text = "Ø£Ù‚ÙˆÙ‰ Ù†Ø³Ø®Ø© Ù„Ø§ ÙŠÙ…ÙƒÙ† Ø§Ù„Ù‡Ø±ÙˆØ¨ Ù…Ù†Ù‡Ø§ ğŸ”¥",
        Duration = 10
    })
end)
